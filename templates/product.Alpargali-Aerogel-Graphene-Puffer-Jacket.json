    "gg_product_detail_Mg4rJH": {
      "type": "gg-product-detail",
      "blocks": {
        "image_banner_6EqhKf": {
          "type": "image_banner",
          "settings": {
            "image_desktop": "shopify://shop_images/aa-detail-b-ib-01.jpg",
            "image_mobile": "shopify://shop_images/aa-detail-b-ib-01-m.jpg",
            "margin_top": 32,
            "margin_bottom": 0,
            "padding_x_desktop": 0,
