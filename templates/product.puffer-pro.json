    "gg_product_detail_Mg4rJH": {
      "type": "gg-product-detail",
      "blocks": {
        "image_banner_6EqhKf": {
          "type": "image_banner",
          "settings": {
            "image_desktop": "shopify://shop_images/1.function-pc.jpg",
