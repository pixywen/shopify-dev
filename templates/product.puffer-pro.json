        },
        "image_banner_MFJcGH": {
          "type": "image_banner",
          "settings": {
            "image_desktop": "shopify://shop_images/4.structure-pc.jpg",
            "image_mobile": "shopify://shop_images/4.structure-m.jpg",
            "margin_top": 0,
